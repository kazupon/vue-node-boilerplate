{
  "root": true,
  "parser": "babel-eslint",
  "extends": "vue",{{#flow}}
  "plugins": ["flowtype"],{{/flow}}
  "rules": {
    {{#flow}}"flowtype/define-flow-type": 1,
    "flowtype/use-flow-type": 1,{{/flow}}
    "object-curly-spacing": ["error", "always"],
    "no-multiple-empty-lines": ["error", { "max": 2, "maxBOF": 1 }]
  }
}
